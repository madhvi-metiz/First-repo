{%- assign og_title = page_title -%}
{%- assign og_url = canonical_url -%}
{%- assign og_type = 'website' -%}
{%- assign og_description = page_description | default: shop.description | default: shop.name -%}
{%- assign og_default_size = '1200x1200' -%}
{%- assign og_default_at_most = 1200 -%}

{%- comment -%}
  Default Open Graph image
{%- endcomment -%}
{%- if settings.default_opengraph_image -%}
  {%- assign og_image = settings.default_opengraph_image -%}
  {%- assign og_image_ratio = og_image.aspect_ratio -%}
  {%- if og_image_ratio > 1 -%}
    {%- assign og_image_width = og_image.width | at_most: og_default_at_most -%}
    {%- assign og_image_height = og_image_width | divided_by: og_image_ratio | round -%}
  {%- elsif og_image_ratio < 1 -%}
    {%- assign og_image_height = og_image.height | at_most: og_default_at_most -%}
    {%- assign og_image_width = og_image_height | times: og_image_ratio | round -%}
  {%- else -%}
    {%- assign og_image_width = og_image.width | at_most: og_default_at_most -%}
    {%- assign og_image_height = og_image.height | at_most: og_default_at_most -%}
  {%- endif -%}
 
  {%- capture og_image_tags -%}
    {%- comment %}<locksmith:46e0>{% endcomment -%}
      {%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta property="og:image" content="http:{{ og_image | img_url: og_default_size }}">{% endif -%} 
      
      {%- comment %}original: <meta property="og:image" content="http:{{ og_image | img_url: og_default_size }}">{%- endcomment %}
    {%- comment %}</locksmith:46e0>{% endcomment -%}
    {%- comment %}<locksmith:e706>{% endcomment -%}
      {%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta property="og:image:secure_url" content="https:{{ og_image | img_url: og_default_size }}">{% endif -%}
      {%- comment %}original: <meta property="og:image:secure_url" content="https:{{ og_image | img_url: og_default_size }}">{%- endcomment %}
    {%- comment %}</locksmith:e706>{% endcomment -%}
    <meta property="og:image:width" content="{{ og_image_width }}">
    <meta property="og:image:height" content="{{ og_image_height }}">
  {%- endcapture -%}
{%- endif -%}

{%- comment -%}
  Template specific overides
{%- endcomment -%}
{%- if template.name == 'product' -%}
  {%- assign og_title = product.title | strip_html -%}
  {%- assign og_type = 'product' -%}
  {%- capture og_image_tags -%}
    {%- for image in product.images limit: 3 -%}
      {%- assign og_image = image -%}
      {%- assign og_image_ratio = og_image.aspect_ratio -%}
      {%- if og_image_ratio > 1 -%}
        {%- assign og_image_width = og_image.width | at_most: og_default_at_most -%}
        {%- assign og_image_height = og_image_width | divided_by: og_image_ratio | round -%}
      {%- elsif og_image_ratio < 1 -%}
        {%- assign og_image_height = og_image.height | at_most: og_default_at_most -%}
        {%- assign og_image_width = og_image_height | times: og_image_ratio | round -%}
      {%- else -%}
        {%- assign og_image_width = og_image.width | at_most: og_default_at_most -%}
        {%- assign og_image_height = og_image.height | at_most: og_default_at_most -%}
      {%- endif -%}
      
      {%- comment %}<locksmith:f89f>{% endcomment -%}
        {%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta property="og:image" content="http:{{ og_image.src | product_img_url: og_default_size }}">{% endif -%}
        {%- comment %}original: <meta property="og:image" content="http:{{ og_image.src | product_img_url: og_default_size }}">{%- endcomment %}
      {%- comment %}</locksmith:f89f>{% endcomment -%}
      {%- comment %}<locksmith:709d>{% endcomment -%}
        {%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta property="og:image:secure_url" content="https:{{ image.src | product_img_url: og_default_size }}">{% endif -%}
        {%- comment %}original: <meta property="og:image:secure_url" content="https:{{ image.src | product_img_url: og_default_size }}">{%- endcomment %}
      {%- comment %}</locksmith:709d>{% endcomment -%}
      <meta property="og:image:width" content="{{ og_image_width }}">
      <meta property="og:image:height" content="{{ og_image_height }}">
    {%- endfor -%}
  {%- endcapture -%}

{%- elsif template.name == 'article' -%}
  {%- assign og_title = article.title | strip_html -%}
  {%- assign og_type = 'article' -%}
  {%- assign og_description = article.excerpt_or_content | strip_html -%}
  {%- if article.image -%}
    {%- assign og_image = article.image -%}
    {%- assign og_image_ratio = og_image.aspect_ratio -%}
    {%- if og_image_ratio > 1 -%}
      {%- assign og_image_width = og_image.width | at_most: og_default_at_most -%}
      {%- assign og_image_height = og_image_width | divided_by: og_image_ratio | round -%}
    {%- elsif og_image_ratio < 1 -%}
      {%- assign og_image_height = og_image.height | at_most: og_default_at_most -%}
      {%- assign og_image_width = og_image_height | times: og_image_ratio | round -%}
    {%- else -%}
      {%- assign og_image_width = og_image.width | at_most: og_default_at_most -%}
      {%- assign og_image_height = og_image.height | at_most: og_default_at_most -%}
    {%- endif -%}

    {%- capture og_image_tags -%}
      {%- comment %}<locksmith:085c>{% endcomment -%}
        {%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta property="og:image" content="http:{{ og_image | img_url: og_default_size }}">{% endif -%}
        {%- comment %}original: <meta property="og:image" content="http:{{ og_image | img_url: og_default_size }}">{%- endcomment %}
      {%- comment %}</locksmith:085c>{% endcomment -%}
      {%- comment %}<locksmith:bb48>{% endcomment -%}
        {%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta property="og:image:secure_url" content="https:{{ og_image | img_url: og_default_size }}">{% endif -%}
        {%- comment %}original: <meta property="og:image:secure_url" content="https:{{ og_image | img_url: og_default_size }}">{%- endcomment %}
      {%- comment %}</locksmith:bb48>{% endcomment -%}
      <meta property="og:image:width" content="{{ og_image_width }}">
      <meta property="og:image:height" content="{{ og_image_height }}">
    {%- endcapture -%}
  {%- endif -%}

{%- elsif template.name == 'collection' -%}
  {%- assign og_title = collection.title | strip_html -%}
  {%- assign og_type = 'product.group' -%}
  {%- if collection.image -%}
    {%- assign og_image = collection.image -%}
    {%- assign og_image_ratio = og_image.aspect_ratio -%}
    {%- if og_image_ratio > 1 -%}
      {%- assign og_image_width = og_image.width | at_most: og_default_at_most -%}
      {%- assign og_image_height = og_image_width | divided_by: og_image_ratio | round -%}
    {%- elsif og_image_ratio < 1 -%}
      {%- assign og_image_height = og_image.height | at_most: og_default_at_most -%}
      {%- assign og_image_width = og_image_height | times: og_image_ratio | round -%}
    {%- else -%}
      {%- assign og_image_width = og_image.width | at_most: og_default_at_most -%}
      {%- assign og_image_height = og_image.height | at_most: og_default_at_most -%}
    {%- endif -%}

    {%- capture og_image_tags -%}
      {%- comment %}<locksmith:0442>{% endcomment -%}
        {%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta property="og:image" content="http:{{ og_image | img_url: og_default_size }}">{% endif -%}
        {%- comment %}original: <meta property="og:image" content="http:{{ og_image | img_url: og_default_size }}">{%- endcomment %}
      {%- comment %}</locksmith:0442>{% endcomment -%}
      {%- comment %}<locksmith:b050>{% endcomment -%}
        {%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta property="og:image:secure_url" content="https:{{ og_image | img_url: og_default_size }}">{% endif -%}
        {%- comment %}original: <meta property="og:image:secure_url" content="https:{{ og_image | img_url: og_default_size }}">{%- endcomment %}
      {%- comment %}</locksmith:b050>{% endcomment -%}
      <meta property="og:image:width" content="{{ og_image_width }}">
      <meta property="og:image:height" content="{{ og_image_height }}">
    {%- endcapture -%}
  {%- endif -%}

{%- elsif template.name == 'password' -%}
  {%- assign og_title = shop.name -%}
  {%- assign og_url = shop.url -%}
  {%- assign og_description = shop.description | default: shop.name -%}
{%- endif -%}

