<script>
    if(typeof(Spurit) === 'undefined'){
      Spurit = {};
    }
    if(!Spurit.tieredPricing){
        Spurit.tieredPricing = {};
    }
    if(!Spurit.tieredPricing.snippet){
        Spurit.tieredPricing.snippet = {};
    }
    Spurit.tieredPricing.snippet.appId = '44';
    Spurit.tieredPricing.snippet.shopHash = 'bfb5babfba6f5cc707921500eb9a0c75';
    Spurit.tieredPricing.snippet.shopDomain = 'white-horse-vapor.myshopify.com';
    Spurit.tieredPricing.snippet.folderStore = 'https://shopify-apps.s3.amazonaws.com/pricing-by-quantity/store/';
    Spurit.tieredPricing.snippet.folderCss = 'https://shopify-apps.s3.amazonaws.com/pricing-by-quantity/css/';
    Spurit.tieredPricing.snippet.moneyFormat = '{{ shop.money_format }}';

    if(typeof(Spurit.globalSnippet) === 'undefined'){
      Spurit.globalSnippet = {
        shop_currency: '{{ shop.currency }}',
        money_format: '{{ shop.money_format }}',
        cart: {{ cart | json }},
        customer_id: '{{ customer.id }}'
      };
    }
</script>
<script src="https://shopify-apps.s3.amazonaws.com/pricing-by-quantity/js/common.js"></script>