<header class="util mobile-only">
  <div class="wrapper flex">
    <div class="search-wrapper">
      {% include 'search-bar' %}
    </div>

    <div class="left-wrapper">
      <ul class="text-links">
        <li class="mobile-menu">
          <a href="#menu" aria-label="Menu" class="toggle-menu menu-link">
            <span class="icon-text">
              <span class="icon icon-menu" aria-hidden="true">{% include 'icon-ui-menu' %}</span>
            </span>
          </a>
        </li>
        <li class="search-li">
          <a href="#" class="search">
            <span class="icon-text">
              <span class="icon icon-search" aria-hidden="true">{% include 'icon-ui-search' %}</span>
              <span class="text search">{{ 'general.search.submit' | t }}</span>
            </span>
          </a>
        </li>
      </ul>
      {% comment %}
        Include currency picker
      {% endcomment %}
      {% if settings.show_multiple_currencies %}
        {% include 'currency-picker' %}
      {% endif %}
    </div>
    <div class="middle-wrapper mobile-only">
      {% assign image = 'WhiteHorseVaporLogo.png' | asset_url %}
      <div class="logo-contain">
        <a href="/" itemprop="url" class="logo-image" aria-label="White Horse Vapor Logo">
          {% include 'logo-wh-vapor' %}
        </a>
      </div>
    </div>
    <div class="right-wrapper">
      {% comment %}
        If customer accounts are enabled, provide login and create account links
      {% endcomment %}
      {% if shop.customer_accounts_enabled %}
        {% include 'accounts-nav' %}
      {% endif %}
      {% comment %}
        Show number of items in the cart and total cost in the /cart link
      {% endcomment %}
      <a href="#" id="container__location-mobile" class="location-switch mobile-only" aria-label="Location">
        <div class="flex-location">
          <div class="flex-item">
            {% include 'icon-ui-location' %}
          </div>
          <div class="flex-item flex-shrink-0">
            <small class="location-info user-state" id="user-state-mobile"></small>
          </div>
        </div>
      </a>
      <a href="#" id="CartButton" aria-label="Cart">
        <span class="header-icon-fallback-text">
          {%- assign cart_icon = 'icon-ui-' | append: settings.cart_icon -%}
          <span class="icon icon-cart icon-cart-mobile" aria-hidden="true">
            {%- include cart_icon %}
            <span id="CartCount" style="{%- if cart.item_count == 0 %} display:none{% endif %}">
              {%- unless cart.item_count == 0 %}{{ cart.item_count }}{% endunless -%}
            </span>
          </span>
        </span>
        <span class="desktop-only">{{ 'layout.cart.title' | t }}</span>
        <span id="CartCost-Mobile" class="money desktop-only">
          {%- unless cart.item_count == 0 %}{{ cart.total_price | money }}{% endunless -%}
        </span>
      </a>
      {% if settings.ajax_cart_enable %}
        {% include 'cart-preview' %}
      {% endif %}
    </div>
  </div>
</header>

