<!doctype html>
<html lang="{{ locale }}" dir="{{ direction }}" class="{{ checkout_html_classes }}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1.0, height=device-height, minimum-scale=1.0, user-scalable=0"
    >
    <meta name="referrer" content="origin">
    <div class="state-div" style="display:none;">{{ checkout.shipping_address.province_code }}</div>
    <div class="customer-div" style="display:none;">
      {% if customer and customer.orders_count > 0 %}
        <p>Same Day Delivery</p>
      {% else %}
        <p>No Same Day Delivery</p>
      {% endif %}
      <p id="customer-logged-in">
        {% if customer %}Logged{% else %}Not Logged{% endif %}
      </p>
    </div>
    <title>{{ page_title }}</title>

    {{ content_for_header }}

    {{ checkout_stylesheets }}
    {{ 'checkout.css' | asset_url | stylesheet_tag }}
    {{ checkout_scripts }}

    {{ 'jquery.min.js' | asset_url | script_tag }}
    {{ 'checkout-order-tracking.js' | asset_url | script_tag }}
    {{ 'checkout-login-text-update.js' | asset_url | script_tag }}

    <script type="text/javascript">
      window.customerTags = {{ customer.tags | json }};
      window.approvedTag = {{ settings.approved_customer_tag | json }}
    </script>

    {% assign agechecker = false %}

    {% for item in checkout.line_items %}
      {% assign agecheck = true %}
      {% if item.title contains 'Gift Card' %}
        {% assign agecheck = false %}
      {% endif %}
      {% if agecheck == true %}
        {% assign agechecker = true %}
      {% endif %}
    {% endfor %}

    {% if agechecker %}
      <style>
        .ac-stripe, #ac-footer, #ac-header, .ac-fa-check {
            display: none !important;
        }
        #agechecker-modal {
            background: linear-gradient(90deg, #0c67c2ec 0%, #064788ec 100%) !important;
        }
        #agechecker-modal #agechecker {
            box-shadow: none !important;
            background-color: transparent !important;
        }
        .ac-button {
            background: white !important;
            border-radius: 500px !important;
            min-width: 0 !important;
            max-width: 200px !important;
            padding: 8px 12px !important;
        }
        .ac-button span, i {
            color: #0c68c2 !important;
            text-transform: uppercase !important;
        }
        .ac-p {
            margin-bottom: 20px !important;
        }
        #ac-continue-button {
            margin-top: 40px !important;
        }
        #agechecker-modal #ac-intro-title, #agechecker-modal .ac-p, #agechecker-modal #agechecker .ac-input .ac-label, #agechecker-modal #agechecker .ac-step .ac-p>*, #agechecker-modal #agechecker .ac-step .ac-p, .ac-hint, .ac-hint strong, .ac-hint span, #agechecker .ac-input .ac-label, #agechecker .ac-step .ac-p, .ac-h3 {
            color: white !important;
        }
        .ac-help {
            color: #c9c9c9 !important;
        }
        #ac-success-text {
            margin-bottom: 0px !important;
            color: white !important;
            font-size: 1.6em !important;
            font-weight: 900 !important;
        }
        #ac-close-button {
            max-width: 225px !important;
            margin-top: 40px !important;
        }
        .ac-h3 {
            font-weight: 900 !important;
            font-size: 36px !important;
            margin: 25px 0 !important;
        }
        .ac-fa-chevron-right {
            display: none !important;
        }
        /* Enter your date of birth */
        #ac-dob .ac-p {
            font-weight: 900 !important;
            font-size: 1.6em !important;
            margin-bottom: 15px !important
        }
        #agechecker #ac-accuracy, #ac-next-button {
            margin-top: 2em !important;
        }
        /* Note: Your date of birth must be accurate... */
        #agechecker #ac-accuracy {
            font-weight: 900 !important;
        }
        #ac-next-button {
            max-width: 150px !important;
        }
        #ac-next-button span {
            font-weight: 900 !important;
        }
        /* DOB Inputs */
        #ac-dob-controls {
            display: flex !important;
            justify-content: center !important;
            gap: 1em !important;
            flex-wrap: wrap;
        }
        #ac-dob .ac-input {
            display: flex !important;
            justify-content: center !important;
        }
        .ac-input-parent {
            display: flex;
            flex-direction: column-reverse;
            gap: 5px;
            align-items: center;
        }
        .ac-input-parent .ac-label {
            min-width: 0px !important;
            text-align: center !important;
            margin-right: 0px !important;
            margin-top: 0.4em !important;
            font-size: 0.8em !important;
        }
        /* Verified Screen */
        #ac-success-text, #ac-step8 > div.ac-hint {
            display: none !important;
        }
        #ac-close-button > span {
            text-transform: none !important;
        }
        #agechecker #ac-inner-container.center {
            padding: 25% 32px;
        }
        @media only screen and (max-width: 700px) {
          #agechecker #ac-inner-container.center {
              padding: 50% 32px;
          }
        }
      </style>
      <noscript><meta http-equiv="refresh" content="0;url=https://agechecker.net/noscript"></noscript>
      {% unless checkout.shipping_method.title == 'WH Retail' %}
        <script>
          (function (w, d) {
            let designSet = false;
            var config = {
              autoload: false,
              onready: function () {
                let ac = AgeCheckerAPI.createInstance({
                  key: 'B4HWCVolp0X03rYwmWuevE0iuNTTu9AI',
                  element: 'div[data-step=payment_method] form .step__footer button',
                  favor_page: true,
                  data: {
                    first_name: '{{checkout.shipping_address.first_name}}',
                    last_name: '{{checkout.shipping_address.last_name}}',
                    address: '{{checkout.shipping_address.address1}}',
                    zip: '{{checkout.shipping_address.zip}}',
                    country: '{{checkout.shipping_address.country_code}}',
                    state: '{{checkout.shipping_address.province_code}}',
                    city: '{{checkout.shipping_address.city}}',
                  },
                  close_all: true,
                  show_close: true,
                  onshow: function () {
                    if(designSet) return;
                    designSet = true;

                    let skipStepZero = setInterval(function() {
                        if(document.querySelector("#ac-step0").style.display.indexOf("block") != -1) {
                            clearInterval(skipStepZero);
                            document.querySelector("#ac-continue-button").click();
                        }
                    }, 10);

                    document.getElementById('ac-hide-button').setAttribute('style', 'visibility: hidden !important');

                    const innerContainer = document.querySelector("#ac-inner-container");
                    const closeButton = document.querySelector("#ac-close-button");
                    const continueButton = document.querySelector("#ac-continue-button");
                    const introTitle = document.querySelector("#ac-intro-title");
                    const dobText = document.querySelector("#ac-dob .ac-p");
                    const dobControls = document.querySelector("#ac-dob-controls");
                    const dobMonth = document.querySelector("#ac-dob-month");
                    const dobDay = document.querySelector("#ac-dob-day");
                    const dobYear = document.querySelector("#ac-dob-year");
                    const verifiedText = document.querySelector("#ac-step8 > div.ac-h3");

                    // Create and insert the logo
                    if(!document.querySelector("#agechecker_custom_logo")) {
                      var logo = document.createElement("div");
                      logo.innerHTML = "<img id='agechecker_custom_logo' src='data:image/png;base64,' style='width: 150px;'>";
                      innerContainer.insertBefore(logo, innerContainer.firstChild);
                    }

                    setInterval(function() {
                        if(introTitle.innerHTML !== "Action Required") {
                            introTitle.innerHTML = "Action Required!";
                        }
                    }, 50);

                    // Change the text of the close and continue button
                    closeButton.children[0].innerHTML = "Submit Order HERE";
                    continueButton.children[0].innerHTML = "Get my vapes!";

                    // Change the text of the verified screen heading
                    verifiedText.innerHTML = "You've been Successfully Verified!"

                    // Change the text of enter your date of birth (Same thing without ':' at the end)
                    dobText.innerHTML = "Enter your date of birth";

                    // Remove any span elements inside of dobControls (The slashes)
                    dobControls.querySelectorAll("span").forEach(span => span.remove());

                    // Remove placeholder text from DOB inputs
                    dobMonth.placeholder = "";
                    dobDay.placeholder = "";
                    dobYear.placeholder = "";

                    // Individually wrap the DOB inputs in a flex column container with labels on the bottom
                    dobMonth.outerHTML = "<div class='ac-input-parent'><label for='ac-dob-month' class='ac-label'>Month</label>" + dobMonth.outerHTML + "</div>";
                    dobDay.outerHTML = "<div class='ac-input-parent'><label for='ac-dob-day' class='ac-label'>Day</label>" + dobDay.outerHTML + "</div>";
                    dobYear.outerHTML = "<div class='ac-input-parent'><label for='ac-dob-year' class='ac-label'>Year</label>" + dobYear.outerHTML + "</div>";

                    let findStepOne = setInterval(function() {
                        if(document.querySelector("#ac-step8").style.display.indexOf("block") != -1) {
                            document.getElementById("agechecker_custom_logo").remove();
                            document.getElementById("ac-inner-container").classList += " center";
                            clearInterval(findStepOne);
                        }
                    }, 10);
                  },
                  onstatuschanged: function (v) {
                    if (v.status) {
                      if (v.status == 'accepted' || v.status == 'photo_id') {
                        var input = document.createElement('input');
                        input.setAttribute('type', 'text');
                        input.setAttribute('name', 'checkout[attributes][age-checker-uuid-tag]');
                        input.setAttribute('value', v.uuid);
                        input.setAttribute('hidden', 'true');
                        document.querySelector('form[data-payment-form]').appendChild(input);
                      }
                      if (v.status == 'photo_id') {
                        document.getElementById('ac-hide-button').click();
                        ac.unbind();
                        setTimeout(function() {
                            document.querySelector('div[data-step=payment_method] form .step__footer button').click();
                        }, 50);
                      }
                    }
                  },
                });
              },
            };

            w.AgeCheckerConfig = config;
            if (config.path && (w.location.pathname + w.location.search).indexOf(config.path)) return;
            var h = d.getElementsByTagName('head')[0];
            var a = d.createElement('script');
            a.src = 'https://cdn.agechecker.net/static/popup/v1/popup.js';
            a.crossOrigin = 'anonymous';
            a.onerror = function (a) {
              w.location.href = 'https://agechecker.net/loaderror';
            };
            h.insertBefore(a, h.firstChild);
          })(window, document);
        </script>
      {% endunless %}
    {% endif %}
    {{ 'checkout.js' | asset_url | script_tag }}
    {{ 'checkout-button-text.js' | asset_url | script_tag }}
  </head>
  <body>
    {{ skip_to_content_link }}
    <div class="oos-overlay">
      <div class="oos-popup">
        <div class="oos-guts">
          <h2><b>Sorry, these items are not available for pick up!</b></h2>
          <p class="unavail-items">Unfortunately these items are not available at our North Providence location</p>
          <div class="oos-items"></div>
          <h3>What would you like to do?</h3>
          <a class="oos-button first" id="ship-it">Deliver my order to me</a>
          <a class="oos-button" id="remove-and-shop">Remove and continue to shop</a>
          <a class="oos-button" id="remove-and-continue">Remove and continue to checkout</a>
        </div>
      </div>
    </div>
