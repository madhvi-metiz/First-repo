    <div class="ReactModalPortal">
      <div class="ReactModal__Overlay Modal">
        <div
          class="ReactModal__Content ReactModal__Content--after-open Modal__content Modal__content--closable"
          tabindex="-1"
          role="dialog"
          aria-modal="true"
        >
          <div class="Modal__body">
            <div class="Modal__close-wrapper">
              <div class="Modal__closer-circle" data-testid="modal-close-icon">
                <svg class="Modal__close-svg" width="24" height="24" viewBox="0 0 24 24">
                  <path fill="#000000" fill-rule="evenodd" d="M12 11.293l5.146-5.147a.5.5 0 0 1 .708.708L12.707 12l5.147 5.146a.5.5 0 0 1-.708.708L12 12.707l-5.146 5.147a.5.5 0 0 1-.708-.708L11.293 12 6.146 6.854a.5.5 0 1 1 .708-.708L12 11.293z"></path>
                </svg>
              </div>
            </div>

            <div class="zipcode-modal">
              <img
                class="icon pop-map"
                src="//cdn.shopify.com/s/files/1/0743/6343/t/149/assets/zipcode.svg?v=129131852618280563541665508379"
              >
              <div class="heading--medium">
                <span data-i18n-key="js.delivery.your_location" class="intl-text"
                  >Your location: <span id="popup-city"></span><span id="popup-state"></span>
                  <span id="popup-zip"></span
                ></span>
              </div>
              <div class="inline-notification success" style="display: block;">
                <div>
                  <span class="inline-notification__body text--body--small">
                    <span class="intl-text pickup">Congratulations! Delivery is available in your area!</span>
                    <span class="intl-text pickup2"></span>
                  </span>
                </div>
              </div>
              <p>
                <span class="intl-text federal"
                  >Due to federal and/or state laws and regulations, pricing and delivery options may vary.</span
                >
              </p>
              <p>
                <span class="intl-text update">If you need to update your location, enter your zip code below:</span>
              </p>
              <div class="text-input-button-container">
                <div class="text-input" id="zip-text-input">
                  <div class="text-input__field-wrapper">
                    <label class="text-input__label" for="text-input-zipcode"
                      ><span data-i18n-key="js.delivery.5_digits_zipcode" class="intl-text"
                        >5 Digit Zip code</span
                      ></label
                    ><input
                      inputmode="numeric"
                      maxlength="5"
                      pattern="[0-9]*"
                      required=""
                      class="text-input__input text--body"
                      id="text-input-zipcode"
                      name="zipcode"
                    >
                  </div>
                </div>
                <button id="updateZip" class="button">
                  <span class="button__text"
                    ><span data-i18n-key="js.delivery.update" class="intl-text">Update</span>
                    <svg class="button__icon" width="14" height="10" fill="none">
                      <path d="M8.235 0L13 5.021 8.235 10M13 5H0" class="icon-stroke"></path>
                    </svg>
                  </span>
                </button>
              </div>
              <p class="green-box" style="display: none;">
                <b>Your location has been updated! Click below to close this window</b>
              </p>
              <hr class="hr hr--modal">
              <button id="close-btn-pop" class="button button--full-width">
                <span class="button__text"
                  ><span data-i18n-key="js.common.modals.close" class="intl-text">Close</span>
                  <svg class="button__icon" width="14" height="10" fill="none">
                    <path d="M8.235 0L13 5.021 8.235 10M13 5H0" class="icon-stroke"></path>
                  </svg>
                </span>
              </button>
            </div>
          </div>
        </div>
      </div>
