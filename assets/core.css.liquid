/* Global Generic Styles */

* {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box; }
  
  * {
    overflow-wrap: break-word;
    word-wrap: break-word;
    word-break: break-word;
}

a {
    -webkit-transition: color 0.1s ease-in-out;
    -moz-transition: color 0.1s ease-in-out;
    transition: color 0.1s ease-in-out;
}
.icon {
    speak: none;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
}
.visibility-hidden,
.visually-hidden {
  position: absolute !important;
  overflow: hidden;
  clip: rect(0 0 0 0);
  height: 1px;
  width: 1px;
  margin: -1px;
  padding: 0;
  border: 0; }
  #page {
    display: unset;
}
p {
    margin: 0 0 0.9375em 0;
}
#menu.panel, #page, .mobile-menu-overlay, header.util, header.promo-bar {
    -webkit-transition: -webkit-transform 0.4s cubic-bezier(0.16, 0.68, 0.43, 0.99);
    -moz-transition: -moz-transform 0.4s cubic-bezier(0.16, 0.68, 0.43, 0.99);
    -o-transition: -o-transform 0.4s cubic-bezier(0.16, 0.68, 0.43, 0.99);
    transition: transform 0.4s cubic-bezier(0.16, 0.68, 0.43, 0.99);
}
article, aside, details, figcaption, figure, footer, header, hgroup, nav, section, summary {
    display: block;
}
header.util.mobile-only li {
    border-right: none;
}
.wrapper.flex .middle-wrapper {
    width: 66.666666%;
}
  @media screen and (max-width:768px) {
   .desktop-only {
     display:none;
   }
   span.promo-bar-text {
    height: 171.2px;
}
   .site-logo.has-image {
    margin-left: auto;
    margin-right: auto;
    display: block;
    text-align: center;
    margin: 0;
    line-height: 0;
    font-size: 0;
}
header.util.mobile-only svg path {
    fill: black;
}
.svg-logo {
    max-width: 120px !important;
}
   header.util .left-wrapper ul.text-links li {
    display: none;
}
header.util .left-wrapper ul.text-links li.mobile-menu {
    display: inline-block;
}
  }

  @media screen and (min-width:769px) {
    .mobile-only {
      display:none !important;
    }
  }
  
