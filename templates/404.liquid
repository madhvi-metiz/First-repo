<!-- /templates/404.liquid -->
<div class="custom-404">
  {% comment %}
    <section class="page page-top">
      <div class="wrapper">
        <header class="content-util">
        {% include 'breadcrumb' %}
        {% include 'social-icons' %}
        </header>
        <header class="page-header">

        </header>
      </div>
    </section>
  {% endcomment %}

  <section class="page">
    <div class="wrapper">
      <div class="content-container narrow rte centered">
        {% comment %} <h1>{{ 'general.404.title' | t }}</h1> {% endcomment %}
        {% comment %} <p>{{ 'general.404.subtext_html' | t }}</p> {% endcomment %}

        <img class="img-404" src="{{ 'wh-404.svg' | asset_url }}" alt="404 image">

        <h1>OOPS You’re Lost...</h1>
        <h4>Don’t STRESS We Gathered Our Most popular Vapes for you.</h4>
      </div>
      {% comment %}
        <div class="content-container narrow rte centered sec-404-popular-item">
          {% section 'featured-collection-slick-404' %}
        </div>
      {% endcomment %}
      <div class="content-container narrow rte centered sec-404-link">
        {% comment %}<h4>Not what you’re Looking for?</h4>{% endcomment %}
        <a class="back-to-home-btn" href="/">Take me home</a>
      </div>
    </div>
  </section>

  <script type="text/javascript">
    window.hdlh = {
      widget_key: '6dupkgr2yfzo9r8hwtf0',
    };
    (function (h, d) {
      var s = d.createElement('script');
      s.type = 'text/javascript';
      s.async = true;
      s.defer = true;
      s.src = h + '?t=' + new Date().getTime();
      d.head.appendChild(s);
    })('https://lighthouse.helpdocs.io/load', document);
  </script>

  {% include 'SEOManager-404' %}
  {% include 'BrokenLinkRedirect' %}
</div>
